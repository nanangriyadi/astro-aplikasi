<layouts>
  <style>
    .navbar {
      box-sizing: border-box;
      outline: none;
      border: none;
      text-decoration: none;
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 0.5rem 7%;
      background-color: rgba(1, 1, 1, 0.8);
      border-bottom: 1px solid #26ad14;
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      z-index: 9999;
    }

    .navbar .navbar-logo {
      box-sizing: border-box;
      outline: none;
      border: none;
      text-decoration: none;
      font-family: "Berkshire Swash", cursive;
      font-size: 2rem;
      font-weight: 800;
      color: #fff;
      font-style: italic;
      text-shadow: 1px 1px 3px rgba(231, 237, 41, 0.5);
    }
    .navbar .navbar-logo span {
      color: #26ad14;
      font-size: 2rem;
      font-weight: 800;
    }

    .navbar .navbar-nav a {
      box-sizing: border-box;
      outline: none;
      border: none;
      text-decoration: none;
      color: #fff;
      display: inline-block;
      font-size: 1.4rem;
      font-weight: bold;
      margin: 1rem;
      text-shadow: 1px 1px 3px rgba(231, 237, 41, 0.5);
    }

    .navbar .navbar-nav a:hover {
      color: #26ad14;
    }

    .navbar .navbar-nav a::after {
      content: "";
      display: block;
      padding-bottom: 0.5rem;
      border-bottom: 0.1rem solid #26ad14;
      transform: scaleX(0);
      transition: 0.3s linear;
    }

    .navbar .navbar-nav a:hover::after {
      transform: scaleX(0.5);
    }
  </style>
