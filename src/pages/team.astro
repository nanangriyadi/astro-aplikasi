<layouts>
  <style>
    .navbar {
      box-sizing: border-box;
      outline: none;
      border: none;
      text-decoration: none;
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 0.5rem 7%;
      background-color: rgba(1, 1, 1, 0.8);
      border-bottom: 1px solid #26ad14;
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      z-index: 9999;
    }

    .navbar .navbar-logo {
      box-sizing: border-box;
      outline: none;
      border: none;
      text-decoration: none;
      font-family: "Berkshire Swash", cursive;
      font-size: 2rem;
      font-weight: 800;
      color: #fff;
      font-style: italic;
      text-shadow: 1px 1px 3px rgba(231, 237, 41, 0.5);
    }
    .navbar .navbar-logo span {
      color: #26ad14;
      font-size: 2rem;
      font-weight: 800;
    }

    .navbar .navbar-nav a {
      box-sizing: border-box;
      outline: none;
      border: none;
      text-decoration: none;
      color: #fff;
      display: inline-block;
      font-size: 1.4rem;
      font-weight: bold;
      margin: 1rem;
      text-shadow: 1px 1px 3px rgba(231, 237, 41, 0.5);
    }

    .navbar .navbar-nav a:hover {
      color: #26ad14;
    }

    .navbar .navbar-nav a::after {
      content: "";
      display: block;
      padding-bottom: 0.5rem;
      border-bottom: 0.1rem solid #26ad14;
      transform: scaleX(0);
      transition: 0.3s linear;
    }

    .navbar .navbar-nav a:hover::after {
      transform: scaleX(0.5);
    }
  </style>
  <nav class="navbar">
    <a href="/" class="navbar-logo">SLBN <span>Badegan</span>Ponorogo.</a>
    <div class="navbar-nav">
      <a href="/">Home</a>
      <a href="/about">Tentang Kami</a>
      <a href="/portofolio">Portofolio</a>
      <a href="/team">Team</a>
      <a href="/contact">Kontak</a>
    </div>
  </nav>

  <layouts>
    <style>
      .team {
        padding: 6rem 7% 1.4rem;
        justify-content: center;
      }

      .team h2 {
        font-family: "Berkshire Swash", cursive;
        text-align: center;
        font-size: 2.6rem;
        margin-bottom: 1.5rem;
        text-shadow: 1px 1px 3px rgba(1, 1, 3, 1);
      }

      .team h2 span {
        color: #26ad14;
        font-size: 2.6rem;
      }

      .team p {
        font-size: 1.8rem;
        margin-top: 0.2rem;
        line-height: 1rem;
        font-size: 200;
        /* text-shadow: 1px 1px 3px rgba(1, 1, 3, 1); */
        color: rgb(14, 15, 15);
        text-align: center;
      }

      .team .container {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        /* max-width: 1200px; */
        margin-block: 2rem;
        gap: 2rem;
      }

      .team .container .card {
        display: flex;
        flex-direction: column;
        width: clamp(15rem, calc(15rem + 1.5vw), 17rem);
        overflow: hidden;
        box-shadow: 0 0.1rem 1rem rgba(0, 0, 0, 0.1);
        border-radius: 1em;
        background: #ece9e6;
        background: linear-gradient(to right, #ffffff, #ece9e6);
        box-shadow: 20px 20px 20px 15px rgba(19, 19, 18, 0.5);
      }

      .team .container .card img {
        max-width: 100%;
        display: block;
        object-fit: cover;
      }

      .team .container .card .card-body {
        padding: 1rem;
        display: flex;
        flex-direction: column;
        gap: 0.5rem;
      }

      .team .container .card .card-body .tag {
        align-self: flex-start;
        padding: 0.25em 0.75em;
        border-radius: 1em;
        font-size: 0.75rem;
      }

      .team .container .card .card-body .tag + .tag {
        margin-left: 0.5em;
      }

      .team .container .card .card-body .tag-blue {
        background: #56ccf2;
        background: linear-gradient(to bottom, #2f80ed, #56ccf2);
        color: #fafafafa;
      }
      .team .container .card .card-body h4 {
        font-size: 1.5rem;
        text-transform: capitalize;
        text-shadow: 1px 1px 3px rgb(209, 177, 22);
        line-height: 1.2;
      }
    </style>
    <section id="team" class="team">
